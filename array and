#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int m;
    cin>>m;
    for(int i=0;i<m;i++)
    {
        int p,k;
        cin>>p>>k;
        int h=a[p];
        for(int j=p+1;j<=k;j++)
        {
            h=h&a[j];
        }
        cout<<h<<endl;
        
    }
}
